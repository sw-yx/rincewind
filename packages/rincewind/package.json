{
  "name": "rincewind",
  "description": "Rincewind: A boilerplate for React + PostCSS (with Tailwind) + TypeScript",
  "version": "3.0.3",
  "author": "swyx @sw-yx",
  "bin": {
    "rincewind": "./bin/run",
    "rw": "./bin/run"
  },
  "bugs": "https://github.com/sw-yx/rincewind/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@parcel/config-default": "^2.0.0-alpha.3.1",
    "@parcel/core": "2.0.0-alpha.3.2",
    "@parcel/fs": "^2.0.0-alpha.3.1",
    "@parcel/package-manager": "^2.0.0-alpha.3.1",
    "chalk": "^3.0.0",
    "copy-template-dir": "^1.4.0",
    "get-port": "^5.0.0",
    "git-user-name": "^2.0.0",
    "tslib": "^1",
    "yarn-or-npm": "^3.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "auto-changelog": "^1.16.2",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "gh-release": "^3.5.0",
    "globby": "^10",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/sw-yx/rincewind",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "rincewind",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "sw-yx/rincewind",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "build": "tsc -b && cp -R src/templates lib",
    "prepack": "rm -rf lib && yarn build && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "preversion": "oclif-dev readme && git add README.md",
    "version": "auto-changelog -p --template keepachangelog && git add CHANGELOG.md",
    "prepublishOnly": "git push && git push --tags && gh-release && yarn copyREADME",
    "copyREADME": "cp README.md ../../README.md"
  },
  "types": "lib/index.d.ts"
}
